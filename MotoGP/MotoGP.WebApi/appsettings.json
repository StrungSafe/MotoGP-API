{
  "AppSettings": {
    "MaxYearsToScrape": 5,
    "MaxDegreeOfParallelism": -1
  },
  "Repository": {
    "Client": {
      "Name": "MotoGP",
      "BaseAddress": "https://api.motogp.pulselive.com/motogp/v1/results/"
    },
    "LocalCache": {
      "Enabled": true,
      "Directory": "C:\\motogp\\repo",
      "Overwrite": false,
      "OverwriteOnError": true
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "MotoGP": "Debug"
    }
  },
  "AllowedHosts": "*"
}
